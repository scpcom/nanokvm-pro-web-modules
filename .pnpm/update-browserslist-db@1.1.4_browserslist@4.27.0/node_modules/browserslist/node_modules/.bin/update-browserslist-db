#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../update-browserslist-db@1.1.4_browserslist@4.27.0/node_modules/update-browserslist-db/cli.js" "$@"
else
  exec node  "$basedir/../../../../../update-browserslist-db@1.1.4_browserslist@4.27.0/node_modules/update-browserslist-db/cli.js" "$@"
fi
