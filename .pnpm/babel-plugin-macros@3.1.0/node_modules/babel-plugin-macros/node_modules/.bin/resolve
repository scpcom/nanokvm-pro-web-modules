#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../resolve@1.22.11/node_modules/resolve/bin/resolve" "$@"
else
  exec node  "$basedir/../../../../../resolve@1.22.11/node_modules/resolve/bin/resolve" "$@"
fi
